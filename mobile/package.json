  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@types/jest": "^29.5.14",
    "@types/react": "~19.1.10",
    "babel-jest": "^30.2.0",
    "babel-preset-expo": "^54.0.6",
    "jest": "^29.7.0",
    "jest-expo": "~54.0.13",
    "typescript": "~5.9.2"
  },
  "jest": {
    "testEnvironment": "node",
    "transform": {
      "^.+\\.(ts|tsx)$": "babel-jest"
    },
    "testMatch": [
      "**/__tests__/**/*.(ts|tsx|js)",
      "**/*.(test|spec).(ts|tsx|js)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.js"
    ],
    "moduleNameMapper": {
      "^../lib/(.*)$": "<rootDir>/lib/$1",
      "^../types$": "<rootDir>/types/index.ts"
    }
  },
  "private": true
}
