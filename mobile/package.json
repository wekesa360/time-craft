    "expo-constants": "~18.0.10",
    "expo-device": "~8.0.9",
    "expo-font": "~14.0.9",
    "expo-haptics": "~15.0.7",
    "expo-image-picker": "~17.0.8",
    "expo-linking": "~8.0.8",
    "expo-local-authentication": "~17.0.7",
    "expo-media-library": "~18.2.0",
    "expo-notifications": "~0.32.12",
    "expo-router": "~6.0.13",
    "expo-secure-store": "~15.0.7",
    "expo-status-bar": "~3.0.8",
