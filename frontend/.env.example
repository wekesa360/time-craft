# Time & Wellness Frontend Environment Variables

# API Configuration
VITE_API_BASE_URL=http://localhost:8787
VITE_API_TIMEOUT=10000

# App Configuration
VITE_APP_NAME=TimeCraft
VITE_APP_VERSION=1.0.0
VITE_APP_ENVIRONMENT=development

# Feature Flags
VITE_ENABLE_SSE=true
VITE_ENABLE_OFFLINE_SYNC=true
VITE_ENABLE_VOICE_PROCESSING=true
VITE_ENABLE_REAL_TIME_UPDATES=true

# External Services
VITE_DEEPGRAM_API_KEY=your_deepgram_api_key_here
VITE_ONESIGNAL_APP_ID=your_onesignal_app_id_here
VITE_GOOGLE_CLIENT_ID=your_google_client_id_here

# Development
VITE_DEBUG_MODE=true
VITE_ENABLE_DEVTOOLS=true
VITE_LOG_LEVEL=debug

# Production
# VITE_API_BASE_URL=https://api.timecraft.app
# VITE_APP_ENVIRONMENT=production
# VITE_DEBUG_MODE=false
# VITE_ENABLE_DEVTOOLS=false
# VITE_LOG_LEVEL=error
